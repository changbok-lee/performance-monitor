{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/api.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/api.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "/login.html",
      "dest": "/frontend/login.html"
    },
    {
      "src": "/url-manager.html",
      "dest": "/frontend/url-manager.html"
    },
    {
      "src": "/index.html",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    }
  ]
}
